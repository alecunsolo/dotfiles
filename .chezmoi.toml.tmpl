{{- $sign := promptString "sign git commits" -}}
{{- $coc := promptString "install Vim coc plugin" -}}
{{- $tmp := promptString "install Tmux plugin manager" -}}

[data.git]
  gpgKey = 'B4C9C5EE9F63A5B7!'
  signCommit = {{ $sign }}

[data.tmux]
  tpm = true
  italics = true

[data.vim]
  coc = {{ $coc }}

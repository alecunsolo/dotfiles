[template]
options=["missingkey=zero"]

[data.git]
  gpgKey = 'B4C9C5EE9F63A5B7!'
  signCommit = false

[data.tmux]
  tpm = true
  italics = true

[data.vim]
  coc = false
  golang = false

[data.mygpg]
  maxttl = 3600
  ttl = 600
{{- if eq .chezmoi.os "darwin" }}
  pinentry = "/usr/local/bin/pinentry-mac"
{{- else }}
  pinentry = "/usr/bin/pinentry"
{{- end }}

[data.mywsl]
  yubipassthrough = false
